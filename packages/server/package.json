{
  "name": "wtp-server",
  "version": "0.0.1",
  "scripts": {
    "start": "bun run build:styles && bun ./src/index.ts",
    "dev": "bun run --watch ./src/index.ts",
    "build": "bun build ./src/index.ts --outdir ./build",
    "build:styles": "npx tailwindcss build -i src/client/styles/styles.css -o src/client/styles/output.css",
    "build:vercel": "bun run build:styles && bun scripts/transform-paths.ts",
    "build:styles:watch": "npm run build:styles --watch",
    "test": "bun test --watch"
  },
  "dependencies": {
    "elysia": "latest",
    "@elysiajs/swagger": "latest",
    "@elysiajs/cron": "latest",
    "@elysiajs/cors": "latest",
    "chalk": "latest",
    "@elysiajs/static": "^0.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@elysiajs/eden": "^0.6.5",
    "react-router": "^6.16.0",
    "react-router-dom": "^6.16.0",
    "tailwindcss-animate": "^1.0.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^1.14.0",
    "@radix-ui/react-dialog": "^1.0.4",
    "@clerk/clerk-react": "^4.24.2",
    "@clerk/themes": "^1.7.5",
    "@shoelace-style/shoelace": "^2.9.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "bun-types": "latest",
    "tailwindcss": "^3.3.3",
    "postcss": "^8.4.29",
    "autoprefixer": "^10.4.15"
  },
  "module": "src/index.js"
}